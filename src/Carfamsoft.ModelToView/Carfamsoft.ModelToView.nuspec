        <dependency id="System.ComponentModel.Annotations" version="5.0.0" exclude="Build,Analyzers"/>
      </group>
      <group targetFramework=".NETStandard2.0">
        <dependency id="Carfamsoft.ModelToView.Shared" version="1.2.0" exclude="Build,Analyzers"/>
        <dependency id="Carfamsoft.ModelToView.ViewAnnotations" version="1.2.0" exclude="Build,Analyzers"/>
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="bin\$configuration$\$id$.xml"/>
